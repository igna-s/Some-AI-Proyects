top_words = set(stopwords.words('spanish'))\n","\n","# Iteramos sobre las reseñas\n","for i in range(len(X)):\n","    review = X[i]  # Obtenemos la reseña original\n","    review = remove_accents(review)  # Transformamos los caracteres acentuados y \"ü\"\n","    # Eliminamos caracteres no alfabéticos, conservando letras, ñ y ü\n","    review = re.sub(r'[^a-zA-ZñÑüÜ]', ' ', review)\n","    review = review.lower()  # Convertimos a minúsculas\n","    review = review.split()  # Dividimos en palabras\n","\n","    # Eliminamos stopwords y aplicamos lematización\n","    review = [lemmatizer.lemmatize(word) for word in review if word not in stop_words]\n","\n","    review = ' '.join(review)  # Reconstruimos la reseña\n","    x_limpio.append(review)  # Agregamos al corpus\n","\n","\n","\n"]},{"cell_type":"code","execution_count":null,"metadata":{"id":"fuBi1YkRrCuO"},"outputs":[],"source":["\n","#Hay palabras mal convertidas, como mas --) ma propio del sistema elegido\n","\n","for i in range(5):\n","    print(f\"x_limpio[{i}]: {x_limpio[i]}\")\n","    print(f\"Y[{i}]: {Y[i]}\")\n","    print(\"-\" * 50)"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":7932,"status":"ok","timestamp":1739986303815,"user":{"displayName":"Ignacio Andrés Schwindt","userId":"08587809665656452662"},"user_tz":180},"id":"o86GT0KKtCh-","outputId":"9100b4a0-7bae-4f55-fd9d-f7bd6447a15f"},"outputs":[{"output_type":"stream","name":"stdout","text":["(50000, 2500)\n","(50000,)\n"]}],"source":["\n","# Utilizo Tfid\n","\n","from sklearn.feature_extraction.text import TfidfVectorizer\n","\n","tf = TfidfVectorizer(max_features = 2500)\n","x = tf.fit_transform(x_limpio).toarray()\n","y=Y\n","\n","print(x.shape)\n","print(y.shape)"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":403,"status":"ok","timestamp":1739986304219,"user":{"displayName":"Ignaci